  }
  delay(500);
  while (!selectNote2){
    step2 = mapSeq();
    syncPhaseInc = step2;
    selectNote2 = true;
  }
  delay(500);
  while (!selectNote3){
    step3 = mapSeq();
    syncPhaseInc = step3;
    selectNote3 = true;
  }
  delay(500);
  while (!selectNote4){
    step4 = mapSeq();
